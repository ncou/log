{
  "name": "phapi/log",
  "description": "Phapi Log contains a Di container validator",
  "keywords": [
    "phapi"
  ],
  "version": "1.0.0",
  "minimum-stability": "stable",
  "license": "MIT",
  "homepage": "http://phapi.github.io",
  "authors": [
    {
      "name": "Peter Ahinko",
      "email": "peter@ahinko.se",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "phapi/contract": "1.*",
    "psr/log": "1.*"
  },
  "suggest": {
    "monolog/monolog": "1.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "mockery/mockery": "0.9.*",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Phapi\\Di\\Validator\\": "src/Phapi/Di/Validator/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Phapi\\Tests\\Di\\Validator\\": "tests/Phapi/Di/Validator/"
    }
  }
}